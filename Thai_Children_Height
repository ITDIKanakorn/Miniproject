                    } else if (hei >= 181 && hei <= 187) {
                        txtResult.setText("tall");
                    } else if (hei >= 161 && hei <= 180) {
                        txtResult.setText("slim");
                    }
                    break;
                case 17:
                    if (hei <= 160) {
                        txtResult.setText("short");
                    } else if (hei >= 181 && hei <= 187) {
                        txtResult.setText("tall");
                    } else if (hei >= 161 && hei <= 180) {
                        txtResult.setText("slimน");
                    }
                    if (hei <= 163) {
                        txtResult.setText("short");
                    } else if (hei >= 183 && hei <= 189) {
