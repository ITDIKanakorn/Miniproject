    private void btnAddActionPerformed(java.awt.event.ActionEvent evt) {                                       
        String id = txtStudentID.getText();
        String name = txtStudentName.getText();
        String surname = txtSurname.getText();
        String midterm = txtMidterm.getText();
        String last = txtFinal.getText();
        if (id.length() > 0 && name.length() > 0 && surname.length() > 0 && midterm.length() > 0 && last.length() > 0) {
            int midtermInt = Integer.valueOf(midterm);
            int lastInt = Integer.valueOf(last);
            sum = lastInt + midtermInt;
            txtTotal.setText("" + sum);
            if (min > sum) {
                min = sum;
            }
            if (max < sum) {
                max = sum;
            }
            Average = Average + sum;
            String Grade;
            if (sum >= 80) {
                Grade = "A";
                A++;
            } else if (sum >= 70) {
                Grade = "B";
                B++;
            } else if (sum >= 60) {
                Grade = "C";
                C++;
            } else if (sum >= 50) {
                Grade = "D";
                D++;
            } else {
                Grade = "F";
                F++;
            }
            txtGrade.setText(Grade);
            i++;
            String totalS = String.valueOf(sum);
            String rowData[] = {id, name, surname, midterm, last, totalS, Grade};
            model.addRow(rowData);
            txtStudentName.setText("");
            txtStudentID.setText("");
            txtSurname.setText("");
            txtMidterm.setText("");
            txtFinal.setText("");
        } else {
            JOptionPane.showMessageDialog(rootPane, "Please select a row.");
        }
    }                                      

    private void btnDeleteActionPerformed(java.awt.event.ActionEvent evt) {                                          
        int row = tblData.getSelectedRow();
        if (row >= 0) {

            int option = JOptionPane.showConfirmDialog(rootPane, "look carefully before deleting");
            if (option == JOptionPane.YES_OPTION) {
                model.removeRow(row);
                i--;
            }
            txtStudentName.setText("");
            txtStudentID.setText("");
            txtSurname.setText("");
            txtMidterm.setText("");
            txtFinal.setText("");
        } else {
            JOptionPane.showMessageDialog(rootPane, "Please select a row.");
        }
    }                                         

    private void btnGradeSummaryActionPerformed(java.awt.event.ActionEvent evt) {                                                
        float sumAverage = Average / i;
        double sumAverage2 = (double) (Math.round(sumAverage * 100.0) / 100.0);
        lblMax.setText("The highest score is ......." + max + "........ score");
        lblMin.setText("The lowest score is ......." + min + "........ score");
        lblAverage.setText("The average score is ......." + sumAverage2 + "........ score");
        lblGradeA.setText("Grade A Amount ........" + A + "........ people");
        lblGradeB.setText("Grade B Amount ........" + B + "........ people");
        lblGradeC.setText("Grade C Amount ........" + C + "........ people");
        lblGradeD.setText("Grade D Amount ........" + D + "........ people");
        lblGradeE.setText("Grade E Amount ........" + E + "........ people");
        int total = A + B + C + D + E;
        lblTotalGrade.setText("Total Amount ......." + total + "....... people");
    }                                               

    private void btnExitActionPerformed(java.awt.event.ActionEvent evt) {                                        
        int option = JOptionPane.showConfirmDialog(rootPane, "Exit? wo ae ae ae ae");
        if (option == JOptionPane.YES_OPTION) {
            System.exit(0);
        }
    }                                       

    private void txtStudentIDKeyTyped(java.awt.event.KeyEvent evt) {                                      
    char c = evt.getKeyChar();
        if (!((c >= '0') && (c <= '9')
                || (c == java.awt.event.KeyEvent.VK_BACK_SPACE)
                || (c == java.awt.event.KeyEvent.VK_DELETE))) {
            getToolkit().beep();
            evt.consume();
        }
    }                                     

